ole.log(err);\r\n        }\r\n    }\r\n\r\n    const configAuthWrapper = {\r\n        headline: 'Email Password'\r\n    };\r\n\r\n    return (\r\n        <AuthWrapper { ...configAuthWrapper }>\r\n            <div className=\"formWrap\">\r\n\r\n                {errors.length > 0 && (\r\n                    <ul>\r\n                        {errors.map((err, index) => {\r\n                            return(\r\n                                <li key={index}>\r\n                                    {err}\r\n                                </li> \r\n                            );\r\n                        })}\r\n                    </ul>\r\n                )}\r\n\r\n                <form onSubmit={handleSubmit}>\r\n                    <FormInput \r\n                        type=\"email\"\r\n                        name=\"email\"\r\n                        value={email}\r\n                        placeholder=\"Email\"\r\n                        handleChange={e => setEmail(e.target.value)}\r\n                    />\r\n                    <Button type=\"submit\">\r\n                        Email Password\r\n                    </Button>\r\n                </form>\r\n            </div>\r\n        </AuthWrapper>\r\n    );\r\n}\r\n\r\nexport default withRouter(EmailPassword);","C:\\Users\\bruno\\OneDrive\\Ambiente de Trabalho\\MyProjects\\ecommerce-website\\src\\redux\\createStore.js",[],"C:\\Users\\bruno\\OneDrive\\Ambiente de Trabalho\\MyProjects\\ecommerce-website\\src\\redux\\rootReducer.js",[],"C:\\Users\\bruno\\OneDrive\\Ambiente de Trabalho\\MyProjects\\ecommerce-website\\src\\redux\\User\\user.reducer.js",[],"C:\\Users\\bruno\\OneDrive\\Ambiente de Trabalho\\MyProjects\\ecommerce-website\\src\\redux\\User\\user.types.js",[],"C:\\Users\\bruno\\OneDrive\\Ambiente de Trabalho\\MyProjects\\ecommerce-website\\src\\redux\\User\\user.actions.js",[],"C:\\Users\\bruno\\OneDrive\\Ambiente de Trabalho\\MyProjects\\ecommerce-website\\src\\pages\\Dashboard\\index.js",[],"C:\\Users\